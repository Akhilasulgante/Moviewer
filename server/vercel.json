{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "/frontend",
      "use": "@vercel/react"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/",
      "dest": "/frontend/build/index.html"
    },
    {
      "src": "/(.+)",
      "dest": "/frontend/build/$1"
    }
  ]
}
